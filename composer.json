{
    "name": "thinframe/command_line",
    "type": "library",
    "homepage": "http://thinframe.net",
    "description": "CLI support for PHP applications.",
    "keywords": ["thinframe", "command line", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sorin Badea",
            "email": "sorin.badea91@gmail.com",
            "homepage": "http://sorinbadea.me",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "thinframe/foundation": "dev-develop",
        "thinframe/pcntl": "dev-develop",
        "thinframe/applications": "dev-develop",
        "league/climate": "2.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "psr-4": {
            "ThinFrame\\CommandLine\\": "src/"
        }
    },
    "bin": [
        "bin/.thinframerc",
        "bin/thinframe-installer",
        "bin/thinframe-completion",
        "bin/thinframe"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev",
            "dev-develop": "0.3-dev"
        }
    }
}
